      },
      "metadata": {
        "version": "1.0.0",
        "description": "YOLO Detection Service - Small Model",
        "protocol": "grpc",
        "capabilities": "detection,tracking",
        "input_formats": "image/jpeg,image/png",
        "output_formats": "json,overlay_image",
        "max_image_size": "4096x4096",
        "gpu_required": "false",
        "health_check": "/health"
      }
